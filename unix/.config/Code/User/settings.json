{
    "editor.fontFamily": "'Fira Code', 'monospace'",
    "editor.cursorStyle": "block-outline",
    "workbench.commandPalette.history": 500,
    "breadcrumbs.enabled": true,
    "terminal.external.linuxExec": "alacritty",
    "terminal.integrated.copyOnSelection": true,
    "terminal.integrated.fontFamily": "Fira Code",
    "terminal.integrated.setLocaleVariables": true,
    "telemetry.enableCrashReporter": false,
    "telemetry.enableTelemetry": false,
    "markdown.preview.fontFamily": "-apple-system, BlinkMacSystemFont, 'Fira Code', 'Segoe WPC', 'Segoe UI', 'HelveticaNeue-Light', 'Ubuntu', 'Droid Sans', sans-serif",
    "python.formatting.provider": "yapf",
    "python.linting.flake8Enabled": true,
    "python.linting.pydocstyleEnabled": true,
    "python.pythonPath": "/home/faris/miniconda3/envs/neovim/bin/python",
    "python.venvPath": "~/miniconda3,~/virtualenvs",
    "code-runner.runInTerminal": true,
    "searchRipgrep.enable": true,
    "vim.neovimPath": "/usr/bin/nvim",
    "vim.leader": "<Space>",
    "vim.enableNeovim": true,
    "vim.sneakUseIgnorecaseAndSmartcase": true,
    "vim.statusBarColorControl": true,
    "vim.timeout": 100,
    "vim.useSystemClipboard": true,
    "git.defaultCloneDirectory": "~/git",
    "git.promptToSaveFilesBeforeCommit": true,
    "debug.inlineValues": true,
    "workbench.colorCustomizations": {
        "statusBar.background": "#005f5f",
        "statusBar.noFolderBackground": "#005f5f",
        "statusBar.debuggingBackground": "#005f5f"
    },
    "editor.suggestFontSize": 6,
    "editor.fontLigatures": true,
    "window.zoomLevel": 2,
    "gitlens.settings.mode": "advanced",
    "emmet.triggerExpansionOnTab": true,
    "git.showPushSuccessNotification": true,
    "workbench.colorTheme": "One Dark Pro",
    "gitlens.resultsExplorer.files.layout": "tree",
    "gitlens.advanced.telemetry.enabled": false,
    "npm.enableScriptExplorer": true,
    "update.enableWindowsBackgroundUpdates": false,
    "editor.renderWhitespace": "boundary",
    "editor.mouseWheelZoom": true,
    "editor.renderLineHighlight": "line",
    "editor.rulers": [
        80,
        120
    ],
    "search.smartCase": true,
    "search.quickOpen.includeSymbols": true,
    "openInGitHub.providerType": "github",
    "php.suggest.basic": false,
    "php.validate.enable": false,
    "python.autoComplete.addBrackets": true,
    "python.linting.pylintEnabled": false,
    "python.linting.pylintUseMinimalCheckers": false,
    "python.terminal.executeInFileDir": true,
    "editor.tabCompletion": true,
    "editor.smoothScrolling": true,
    "python.unitTest.autoTestDiscoverOnSaveEnabled": false,
    "python.unitTest.promptToConfigure": false,
    "git.autofetch": true,
}