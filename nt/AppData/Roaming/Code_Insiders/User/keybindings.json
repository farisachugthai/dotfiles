[
  {
    "command": "vim.showQuickpickCmdLine",
    "key": "ctrl+shift+; shift+alt+;"
  },
  {
    "command": "-workbench.action.terminal.openNativeConsole",
    "key": "ctrl+shift+c",
    "when": "!terminalFocus"
  },
  {
    "command": "-workbench.action.closeWindow",
    "key": "ctrl+w",
    "when": "!editorIsOpen && !multipleEditorGroups"
  },
  {
    "command": "-workbench.action.closeWindow",
    "key": "ctrl+shift+w"
  },
  {
    "command": "-workbench.action.terminal.focusFindWidget",
    "key": "ctrl+f",
    "when": "terminalFocus"
  },
  {
    "command": "restructuredtext.preview.refresh",
    "key": "f5",
    "when": "editorLangId == 'restructuredtext'"
  },
  {
    "command": "workbench.action.closeAllEditors",
    "key": "Ctrl+F4",
    "when": "editorFocus"
  },
  {
    "command": "workbench.action.terminal.kill",
    "key": "Ctrl+F4",
    "when": "terminalFocus"
  }
  {
    "args": {
      "cwd": "${fileDirname}"
    },
    "command": "workbench.action.terminal.newWithCwd",
    "key": "Meta+Shift+h"
  },
  {
    "command": "-workbench.action.closeAllEditors",
    "key": "Ctrl+K Ctrl+W"
  },
  {
    "command": "editor.action.marker.nextInFiles",
    "key": "ctrl+]",
    "when": "editorFocus && !editorReadonly"
  },
  {
    "command": "-editor.action.marker.nextInFiles",
    "key": "F8",
    "when": "editorFocus && !editorReadonly"
  },
  {
    "command": "editor.action.marker.prevInFiles",
    "key": "[+q",
    "when": "editorFocus && !editorReadonly"
  },
  {
    "key": "ctrl+k ctrl+k",
    "command": "editor.action.defineKeybinding",
    "when": "editorTextFocus"
  },
  {
    "command": "-editor.action.marker.prevInFiles",
    "key": "Shift+F8",
    "when": "editorFocus && !editorReadonly"
  },
  {
    "command": "-workbench.actions.view.problems",
    "key": "Ctrl+Shift+M"
  },
  {
    "command": "selectNextSuggestion",
    "key": "Ctrl+j",
    "when": "editorTextFocus && suggestWidgetMultipleSuggestions && suggestWidgetVisible"
  },
  {
    "command": "selectPrevSuggestion",
    "key": "Ctrl+k",
    "when": "editorTextFocus && suggestWidgetMultipleSuggestions && suggestWidgetVisible"
  },
  {
    "command": "-workbench.action.terminal.focusFindWidget",
    "key": "Ctrl+f",
    "when": "-workbench.action.terminal.focusFindWidget"
  },
  {
    "command": "workbench.action.quickOpenNavigateNext",
    "key": "Ctrl+j",
    "when": "inQuickOpen"
  },
  {
    "command": "workbench.action.quickOpenNavigatePrevious",
    "key": "Ctrl+k",
    "when": "inQuickOpen"
  },
  {
    "command": "workbench.action.focusLeftGroup",
    "key": "Ctrl+h"
  },
  {
    "command": "workbench.action.focusAboveGroup",
    "key": "Ctrl+j",
    "when": "!suggestWidgetMultipleSuggestions && !inQuickOpen"
  },
  {
    "command": "workbench.action.focusBelowGroup",
    "key": "Ctrl+k",
    "when": "!suggestWidgetMultipleSuggestions && !inQuickOpen"
  },
  {
    "command": "workbench.action.focusRightGroup",
    "key": "Ctrl+l"
  },
  {
    "command": "workbench.action.splitEditorDown",
    "key": "Ctrl+W s"
  },
  {
    "command": "workbench.action.closeActiveEditor",
    "key": "Ctrl+W c"
  },
  {
    "command": "workbench.action.closeEditorsInOtherGroups",
    "key": "Ctrl+W o"
  },
  {
    "command": "workbench.action.splitEditorRight",
    "key": "Ctrl+W v"
  },
  {
    "command": "editor.action.marker.prev",
    "key": "Ctrl+[",
    "when": "editorTextFocus && !editorReadonly"
  },
  {
    "command": "restructuredtext.showPreview",
    "key": "Ctrl+Enter",
    "when": "editorTextFocus && editorLangId == 'restructuredtext'"
  },
  {
    "args": "-m IPython",
    "command": "python.createTerminal",
    "key": "ctrl+`",
    "when": "!terminalFocus && editorLangId == 'python'"
  },
  {
    "command": "python.execInTerminal",
    "key": "Ctrl+Enter",
    "when": "editorTextFocus && editorLangId == 'python'"
  },
  {
    "command": "python.execSelectionInTerminal",
    "key": "Ctrl+Enter",
    "when": "editorTextFocus && !editorReadonly && editorLangId == 'python'"
  },
  {
    "command": "-workbench.action.splitEditor",
    "key": "Ctrl+\\"
  },
  {
    "command": "workbench.action.toggleMaximizedPanel",
    "key": "Ctrl+\\"
  },
  {
    "command": "python.datascience.notebookeditor.runallcells",
    "key": "f5",
    "when": "editorTextFocus && python.datascience.featureenabled && editorLangId == 'python' && python.datascience.hascodecells && !editorHasSelection"
  },
  {
    "command": "python.debugTestNode",
    "key": "f5",
    "when": "editorTextFocus && !inDebugRepl && editorLangId == 'python'"
  },
  {
    "command": "-repl.action.filter",
    "key": "ctrl+f",
    "when": "inDebugRepl && textInputFocus"
  },
  {
    "command": "-workbench.action.terminal.clearSelection",
    "key": "escape",
    "when": "terminalFocus && terminalTextSelected && !terminalFindWidgetVisible"
  },
  {
    "command": "-workbench.action.terminal.navigationModeExit",
    "key": "escape",
    "when": "accessibilityModeEnabled && terminalA11yTreeFocus"
  },
  {
    "command": "-editor.action.diffReview.next",
    "key": "f7",
    "when": "isInDiffEditor"
  },
  {
    "command": "-workbench.action.quickOpen",
    "key": "ctrl+p"
  },
  {
    "command": "-workbench.action.quickOpen",
    "key": "ctrl+e"
  },
  {
    "command": "workbench.action.terminal.runActiveFile",
    "key": "shift+f5",
    "when": "!editorReadOnly && !inDebugRepl"
  },
  {
    "command": "python.viewOutput",
    "key": "ctrl+g ctrl+p"
  },
  {
    "command": "workbench.action.openActiveLogOutputFile",
    "key": "ctrl+g l"
  },
  {
    "command": "git.showOutput",
    "key": "ctrl+g alt+g"
  },
  {
    "command": "editor.action.fontZoomOut",
    "key": "ctrl+-"
  },
  {
    "command": "deleteFile",
    "key": "delete",
    "when": "explorerViewletVisible && filesExplorerFocus && !inputFocus"
  },
  {
    "command": "-deleteFile",
    "key": "delete",
    "when": "explorerViewletVisible && filesExplorerFocus && !explorerResourceMoveableToTrash && !explorerResourceReadonly && !inputFocus"
  },
  {
    "command": "editor.action.peekDeclaration",
    "key": "ctrl+shift+]",
    "when": "editorTextFocus"
  },
  {
    "command": "workbench.action.closeWindow",
    "key": "Ctrl+F4",
    "when": "terminalFocus"
  },
  {
    "command": "python.debugTestNode",
    "key": "f5",
    "when": "editorTextFocus && !inDebugRepl && editorLangId == 'python'"
  },
  {
    "command": "workbench.action.openNextRecentlyUsedEditor",
    "key": "Ctrl+w n",
    "when": "editorTextFocus"
  },
  {
    "command": "workbench.action.openPreviousRecentlyUsedEditor",
    "key": "Ctrl+w p",
    "when": "editorTextFocus"
  },
  {
    "command": "-workbench.action.openNextRecentlyUsedEditor",
    "key": "Ctrl-Tab"
  },
  {
    "command": "-workbench.action.openPreviousRecentlyUsedEditor",
    "key": "Ctrl-Shift-Tab"
  },
  {
    "command": "workbench.action.focusRightGroup",
    "key": "Ctrl+w l",
    "when": "editorTextFocus"
  },
  {
    "command": "workbench.action.focusLeftGroup",
    "key": "Ctrl+w h",
    "when": "editorTextFocus"
  },
  {
    "command": "workbench.action.focusAboveGroup",
    "key": "Ctrl+w k",
    "when": "editorTextFocus"
  },
  {
    "command": "workbench.action.focusBelowGroup",
    "key": "Ctrl+w j",
    "when": "editorTextFocus"
  },
  {
    "command": "workbench.action.showCommands",
    "key": "Alt+x"
  },
  //{
  //  "command": "editor.action.defineKeybinding",  // to remove the conditional aspect from the builtin
  //  "key": "Ctrl+k Ctrl+k"
  //},
  {
    //"command": "workbench.action.openDefaultKeybindingsFile"
    "command": "workbench.action.openGlobalKeybindings",
    "key": "Ctrl+k Ctrl+k"
  },
  {
    "command": "-workbench.action.files.saveAs",
    "key": "Ctrl-Shift-s"
  },
  {
    "command": "workbench.action.files.saveAll",
    "key": "Ctrl-Shift-s"
  }
]